{
  "routes": [
    {
      "route": "/about",
      "rewrite": "/src/about.html"
    },
    {
      "route": "/services",
      "rewrite": "/src/services.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/src/index.html",
    "exclude": ["/assets/*", "*.{css,scss,js,png,gif,ico,jpg,jpeg,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".css": "text/css",
    ".js": "application/javascript",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".ttf": "font/ttf",
    ".eot": "application/vnd.ms-fontobject"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/src/index.html",
      "statusCode": 200
    }
  }
}
